<% if !current_user.rank.can_edit %>
  <div>
	  Du besitzt nicht die n√∂tigen Rechte, um <%= current_user.alliance.name %> zu bearbeiten.
  </div>
<% else %>
  <h1> <%= current_user.alliance.name %> bearbeiten: </h1>
  <table>
	  <tbody>
	  	<% self.ranks.each do |rank| %>		  	  
	 	    <tr>	
	  	      <td>rank.name</td>  	    
	          <td><%= link_to 'Edit', edit_rank_path(rank) %></td>
	          <td><%= link_to 'Destroy', rank, method: :delete, data: { confirm: 'Delete this rank?' } %></td>
	  	    </tr>	  	  
	  	<end>
	  	<td><%= link_to 'Create new rank', new_rank_path(rank) %></td>	
	  </tbody>
  </table>	
<% end %>